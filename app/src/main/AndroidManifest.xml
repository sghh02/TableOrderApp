<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.websarva.wings.android.tableorderapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.TableOrderApp">
        <activity
            android:name=".admin.editmenu.MenuAddActivity"
            android:exported="false" />
        <activity
            android:name=".admin.editmenu.MenuEditActivity"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name=".customer.customerstart.CustomerStartActivity"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name=".customer.ordermenu.OrderActivity"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SelectionActivity"
            android:exported="true"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".provider.ProductProvider"
            android:authorities=" com.websarva.wings.android.tableorderapp.provider"
            android:exported="false" />
    </application>

</manifest>